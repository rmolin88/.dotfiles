#!/usr/bin/env python

import subprocess
import shlex
import json

cmd = shlex.split(
    'curl -vs http://192.168.128.199/admin/api.php\?summaryRaw | json_pp')
result = subprocess.run(
        cmd,
        stdout=subprocess.PIPE,
        stdin=subprocess.PIPE,
        stderr=subprocess.PIPE)
data = json.dumps(result.stdout.decode('utf-8'))
#  print(data)
#  print(json.dumps(data))
jdata = json.loads(data)
idx = jdata.find('ads_blocked_today')
print(idx)


