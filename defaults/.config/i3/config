# Mode Resize window (you can also use the mouse for that){{{
	mode "resize" {
# These bindings trigger as soon as you enter the resize mode

# Pressing left will shrink the windowâ€™s width.
# Pressing right will grow the windowâ€™s width.
# Pressing down will shrink the windowâ€™s height.
# Pressing up will grow the windowâ€™s height.
		bindsym $left       resize shrink width 10 px or 10 ppt
			bindsym $up       resize grow height 10 px or 10 ppt
			bindsym $down         resize shrink height 10 px or 10 ppt
			bindsym $right      resize grow width 10 px or 10 ppt

# same bindings, but for the arrow keys
			bindsym Left        resize shrink width 10 px or 10 ppt
			bindsym Down        resize grow height 10 px or 10 ppt
			bindsym Up          resize shrink height 10 px or 10 ppt
			bindsym Right       resize grow width 10 px or 10 ppt

# back to normal: Enter or Escape
			bindsym Return mode "default"
			bindsym Escape mode "default"
	}
# }}}

# Mode bluetooth {{{
#
	set $mode_bluetooth bluetooth to: (w)wh-1000xm2, (b)ose (d)isconnect
		bindsym mod1+control+b mode "$mode_bluetooth"
		mode "$mode_bluetooth" {
			bindsym w mode "default", exec --no-startup-id \
				~/.config/i3/scripts/bluetooth.lua "connect" "04:5D:4B:EA:9C:B2"
				bindsym b mode "default", exec --no-startup-id \
				~/.config/i3/scripts/bluetooth.lua "connect" "2C:41:A1:25:F5:C1"
				bindsym d mode "default", exec --no-startup-id \
				~/.config/i3/scripts/bluetooth.lua "disconnect"

				bindsym Return mode "default"
				bindsym Escape mode "default"
		}
# }}}

# Mode Password{{{
#
	set $mode_password pass: press (r)root, (e)esilio, (h)github, (g)git, (l)gitlab, (d)igital_ocean, (s)urbook
		bindsym mod1+control+w mode "$mode_password"
		mode "$mode_password" {
			bindsym r mode "default", \
				exec --no-startup-id pass --clip linux/`hostname`/user/root
				bindsym h mode "default", exec --no-startup-id \
				pass --clip linux/`hostname`/ssh_key/id_ed25519_github
				bindsym d mode "default", exec --no-startup-id \
				pass --clip linux/`hostname`/ssh_key/id_ed25519_digitalocean
				bindsym g mode "default", exec --no-startup-id \
				pass --clip linux/`hostname`/ssh_key/id_ed25519_git_server
				bindsym l mode "default", exec --no-startup-id \
				pass --clip linux/`hostname`/ssh_key/id_ed25519_gitlab
				bindsym e mode "default", exec --no-startup-id \
				pass --clip linux/`hostname`/resilio/admin
				bindsym s mode "default", exec --no-startup-id \
				pass --clip linux/`hostname`/ssh_key/id_ed25519_surbook

				bindsym Return mode "default"
				bindsym Escape mode "default"
		}
# }}}

# Mode gaps{{{
	set $mode_gaps Gaps: (o) outer, (i) inner
		set $mode_gaps_outer Outer Gaps: +|-|0 (local), Shift + +|-|0 (global)
		set $mode_gaps_inner Inner Gaps: +|-|0 (local), Shift + +|-|0 (global)
		bindsym $mod+Shift+g mode "$mode_gaps"

		mode "$mode_gaps" {
			bindsym o      mode "$mode_gaps_outer"
				bindsym i      mode "$mode_gaps_inner"
				bindsym Return mode "default"
				bindsym Escape mode "default"
		}

	mode "$mode_gaps_inner" {
		bindsym plus  gaps inner current plus 5
			bindsym minus gaps inner current minus 5
			bindsym 0     gaps inner current set 0

			bindsym Shift+plus  gaps inner all plus 5
			bindsym Shift+minus gaps inner all minus 5
			bindsym Shift+0     gaps inner all set 0

			bindsym Return mode "default"
			bindsym Escape mode "default"
	}
	mode "$mode_gaps_outer" {
		bindsym plus  gaps outer current plus 5
			bindsym minus gaps outer current minus 5
			bindsym 0     gaps outer current set 0

			bindsym Shift+plus  gaps outer all plus 5
			bindsym Shift+minus gaps outer all minus 5
			bindsym Shift+0     gaps outer all set 0

			bindsym Return mode "default"
			bindsym Escape mode "default"
	}
# }}}

# Mode Screenshot: {{{
# Sat Dec 30 2017 22:56:
# bindsym f exec --no-startup-id gnome-screenshot -f \
# ~/.cache/`date '+%y-%m-%d_%T'`.png;mode "default"
# bindsym s exec --no-startup-id gnome-screenshot -a -f \
# ~/.cache/`date '+%y-%m-%d_%T'`.png;mode "default"
# bindsym w exec --no-startup-id gnome-screenshot -w -f \
# ~/.cache/`date '+%y-%m-%d_%T'`.png;mode "default"
	set $mode_screenshot screenshot: [f]ull_screen, [s]election, [w]indow, [g]flameshot_gui
	bindsym $mod+Shift+s mode "$mode_screenshot"

	mode "$mode_screenshot" {
		bindsym f exec --no-startup-id sleep 0.2 && scrot '%Y-%m-%d_%T_scrot.png' \
			-e 'mv $f ~/.cache/';mode "default"
			bindsym s exec --no-startup-id sleep 0.2 && scrot -s \
			'%Y-%m-%d_%T_scrot.png' \
			-e 'mv $f ~/.cache/';mode "default"
			bindsym w exec --no-startup-id sleep 0.2 && scrot -u \
			'%Y-%m-%d_%T_scrot.png' \
			-e 'mv $f ~/.cache/';mode "default"
			bindsym g exec --no-startup-id flameshot gui; mode "default"

			bindsym Escape mode "default"
			bindsym Return mode "default"
	}
# }}}

