		# exec --no-startup-id ~/.config/i3/scripts/focus_previous.sh
		# exec --no-startup-id guake
    exec --no-startup-id if ! pgrep -u "$USER" ssh-agent > /dev/null; then\
          ssh-agent > "$XDG_RUNTIME_DIR/ssh-agent.env" \
        fi


