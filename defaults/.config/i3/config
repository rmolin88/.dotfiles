# Bindigs{{{
# Note: Use Mouse+$mod to drag floating windows to their wanted position

# now if I want to write a bunch of stuff I can do that with dvorak
	bindsym control+mod1+d exec --no-startup-id \
		~/.config/i3/scripts/switch_dvorak.sh

# Launch toggle scratchpads
bindsym $mod+u exec --no-startup-id \
  ~/.config/i3/scripts/xdotool_launch glances "glances --theme-white"
bindsym $mod+p exec --no-startup-id \
  ~/.config/i3/scripts/xdotool_launch htop htop
bindsym $mod+n exec --no-startup-id \
  ~/.config/i3/scripts/xdotool_launch scratch "~/.config/tmux/scratch.sh"
bindsym $mod+i exec --no-startup-id \
  ~/.config/i3/scripts/xdotool_launch terminal \
  "-o background_opacity=0.9 tmux"
bindsym $mod+o exec --no-startup-id \
  ~/.config/i3/scripts/xdotool_launch neomutt "~/.config/tmux/neomutt.sh"


